{
  "name": "baynavigator",
  "type": "module",
  "version": "2.0.0",
  "description": "Bay Area community resources - Free and low-cost programs",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "generate-api": "node scripts/generate-api.cjs && node scripts/generate-geojson.cjs",
    "setup": "node scripts/setup.cjs",
    "verify:gate": "node scripts/verify-gate.cjs",
    "test": "npx playwright test",
    "test:unit": "node --test tests/unit/*.test.cjs",
    "test:a11y": "npx playwright test --project=accessibility",
    "test:all": "npm run test:unit && npm run test"
  },
  "keywords": [
    "bay-area",
    "community",
    "resources",
    "assistance"
  ],
  "author": "Bay Tides",
  "license": "MIT",
  "dependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.16.6",
    "fuse.js": "^7.1.0",
    "js-yaml": "^4.1.1",
    "maplibre-gl": "^5.15.0",
    "pmtiles": "^4.3.2",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@playwright/test": "^1.57.0",
    "@types/js-yaml": "^4.0.9"
  }
}
